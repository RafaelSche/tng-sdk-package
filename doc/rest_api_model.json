{"definitions": {"PackagesStatusListGetReturn": {"type": "object", "properties": {"package_processes": {"items": {"$ref": "#/definitions/PackagesStatusItemGetReturn"}, "type": "array"}}}, "PackagesStatusItemGetReturn": {"properties": {"package_process_uuid": {"description": "UUID of started unpackaging process.", "type": "string"}, "status": {"description": "Status of the unpacking process: waiting|runnig|failed|done", "type": "string"}, "error_msg": {"description": "More detailed error message.", "type": "string"}}, "type": "object", "required": ["package_process_uuid", "status"]}, "PingGetReturn": {"properties": {"alive_since": {"description": "system uptime", "type": "string"}}, "type": "object", "required": ["alive_since"]}}, "host": "tng-package.5gtango.eu", "info": {"description": "5GTANGO tng-package REST API to package/unpacke NFV packages.", "version": "0.1", "title": "5GTANGO tng-package API"}, "basePath": "http://tng-package.5gtango.eu/api", "paths": {"/v1/packages": {"post": {"parameters": [{"name": "package", "description": "Uploaded package file", "type": "file", "required": true, "in": "formData"}, {"name": "callback_url", "description": "URL called after unpackaging (optional)", "type": "string", "in": "formData"}, {"name": "layer", "description": "Layer tag to be unpackaged (optional)", "type": "string", "in": "formData"}, {"name": "format", "description": "Package format (optional)", "type": "string", "default": "eu.5gtango", "in": "formData"}, {"name": "skip_store", "description": "Skip catalog upload of contents (optional)", "type": "boolean", "default": false, "in": "formData"}, {"name": "X-Fields", "format": "mask", "description": "An optional fields mask", "type": "string", "in": "header"}], "consumes": ["multipart/form-data"], "tags": ["v1"], "operationId": "post_packages", "responses": {"400": {"description": "Bad package: Could not unpackage given package."}, "200": {"description": "Success", "schema": {"$ref": "#/definitions/PackagesStatus